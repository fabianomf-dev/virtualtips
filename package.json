    "@types/react": "18.2.30",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.4.16",
    "axios": "^1.5.1",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.6",
    "mongodb": "^6.1.0",
    "next": "13.5.6",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "request-ip": "^3.3.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.7.1",
